import java.util.*;

public class Main {
  public static void main(String[] args){
   
    float num1 = 3.14f;
    float num2 = 2.5f;
    
    // Multiplying two float variables
    float result = num1 * num2;
    
    // Printing the final result
    System.out.println("The product of " + num1 + " and " + num2 + " is " + result);
  }
}